{
  "name": "resume-extractor",
  "version": "1.0.1",
  "description": "resume extractor package. Extract resume sections into structured JSON",
  "scripts": {
    "build": "tsc",
    "ci": "npm run build && npm run check-format && npm run test",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "test": "vitest run",
    "dev": "vitest",
    "local-release": "changeset version && changeset publish",
    "prepublishOnly": "npm run ci"
  },
  "keywords": [
    "resume",
    "parsing",
    "ai",
    "extraction",
    "structured data"
  ],
  "homepage": "https://github.com/mohamed-elzohery/resume-extractor",
  "bugs": {
    "url": "https://github.com/mohamed-elzohery/resume-extractor/issues"
  },
  "author": "Mohamed Elzohery <mohamed.sayed.elzohery@gmail.com> (https://elzohery.vercel.app.com)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mohamed-elzohery/resume-extractor.git"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "dist/index.js",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@types/node": "^24.5.2",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "dotenv": "^17.2.2",
    "openai": "^6.3.0",
    "zod": "^3.25.76"
  }
}
